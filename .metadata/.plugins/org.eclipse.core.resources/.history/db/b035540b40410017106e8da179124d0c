package com.exercise.java;

public class UserInfo {
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getQx() {
		return qx;
	}

	public void setQx(String qx) {
		this.qx = qx;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	private String name;
	private String pwd;
	private String qx;
	private String id;
	
	public UserInfo(){
		
	}
	
	public UserInfo(String name,  String pwd) {
		this.name = name;
		this.pwd = pwd;
	}
	
	
}
