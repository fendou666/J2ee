		<%
		ArrayList<Message> msgList = (ArrayList<Message>)application.getAttribute("msg");
			if(msgList==null){
				out.write("<tr><td>用户提交留言服务端设置属性失败</td></tr>");
			}else{
				Iterator<UserInfo> iterator = msgList.iterator()
				while(iterator.hasNext()){
					out.write("<tr><td>" + iterator.next().getName() + "</td></tr>");
				}				
			}
		%>
