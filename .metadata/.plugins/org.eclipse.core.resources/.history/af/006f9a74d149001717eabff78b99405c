<%@page import="java.util.Iterator"%>
<%@page import="com.study.mvc.model.javabean.StudentInfo"%>
<%@page import="java.util.List"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Insert title here</title>
</head>
<body>
	<form>
		<tr>
			<td>序号</td>
			<td>学号</td>
			<td>姓名</td>
			<td>性别</td>
			<td>年龄</td>
			<td>毕业院校</td>
		</tr>
		<% 
			List<StudentInfo> stuList = (List<StudentInfo>)session.getAttribute("stuList");
			if(stuList==null){
				%>
				<tr>
					<td colspan="6">没有获取到信息</td>
				</tr>
				<% 
			}else{
				Iterator<StudentInfo> iterator = stuList.iterator();
				StudentInfo tmp = null;
				out.write("<tr>");
				while(iterator.hasNext()){
					tmp = iterator.next();
					out.write("<td><%=tmp.get %></td>"
						+"<td>学号</td><td>姓名</td><td>性别</td><td>年龄</td><td>毕业院校</td>");
				}
				while(iterator.hasNext()){
					tmp = iterator.next();
					tmp.getId();
					tmp.getNum();
					tmp.getName();
					tmp.getSex();
					tmp.getAge();
					tmp.getSchool();
					out.write("<td><%=tmp.getId() %></td>"
						+"<td><%=tmp.getNum() %></td>"
						+ "<td><%%></td>"
						+ "<td>性别</td>"
						+ "<td>年龄</td>"
						+ "<td>毕业院校</td>");
				}
				out.write("</tr>");
			}
		%>
	</form>
</body>
</html>